{
  "name": "shlink-dashboard",
  "description": "",
  "main": "build/server.js",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "react-router build && tsc --project tsconfig.build-server.json && cp -R app/db/migrations build/migrations && cp migrations.config.ts build/migrations.config.ts && tsx scripts/replace-version.ts",
    "build:dist": "node --run build && tsx ./scripts/create-dist-file.ts",
    "test": "vitest run",
    "test:ci": "npm run test -- --coverage",
    "lint": "eslint app scripts test server.ts *.config.{js,ts}",
    "lint:fix": "npm run lint -- --fix",
    "types": "react-router typegen && tsc",
    "start": "tsx ./server.ts",
    "preview": "NODE_ENV=production tsx ./server.ts",
    "migration:create": "./node_modules/tsx/dist/cli.mjs ./node_modules/@mikro-orm/cli/esm migration:create --config ./migrations.config.ts",
    "migration:run": "./node_modules/tsx/dist/cli.mjs ./node_modules/@mikro-orm/cli/esm migration:up --config ./migrations.config.ts",
    "migration:down": "./node_modules/tsx/dist/cli.mjs ./node_modules/@mikro-orm/cli/esm migration:down --config ./migrations.config.ts",
    "password:hash": "tsx ./scripts/hash-password.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
