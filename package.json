  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "test": "vitest run",
    "test:ci": "npm run test -- --coverage",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext .js,.ts,.tsx app",
    "lint:fix": "npm run lint:js:fix",
    "lint:js:fix": "npm run lint:js -- --fix",
    "types": "tsc",
    "start": "tsx ./server.ts",
    "preview": "NODE_ENV=production tsx ./server.ts",
    "migration:run": "tsx node_modules/.bin/typeorm migration:run -d app/db/data-source.server.ts",
    "migration:revert": "tsx node_modules/.bin/typeorm migration:revert -d app/db/data-source.server.ts",
    "migration:create": "tsx node_modules/.bin/typeorm migration:create app/db/migrations/migration"
  },
  "dependencies": {
