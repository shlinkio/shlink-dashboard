    "@mikro-orm/migrations": "^6.6.1",
    "@mikro-orm/mssql": "^6.6.1",
    "@mikro-orm/mysql": "^6.6.1",
    "@mikro-orm/postgresql": "^6.6.1",
    "@react-router/express": "^7.9.6",
    "@react-router/node": "^7.9.6",
    "@shlinkio/shlink-frontend-kit": "^1.3.1",
    "@shlinkio/shlink-js-sdk": "^3.0.1",
    "@shlinkio/shlink-web-component": "^0.17.0",
    "argon2": "^0.44.0",
