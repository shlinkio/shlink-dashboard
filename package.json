    "tsx": "^4.20.6",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.6",
    "@shlinkio/eslint-config-js-coding-standard": "~3.7.0",
    "@stylistic/eslint-plugin": "^5.6.1",
